				float3 ase_worldNormal = IN.ase_texcoord3.xyz;
				float dotResult12 = dot( ase_worldNormal , _MainLightPosition.xyz );
				float3 temp_cast_0 = (( pow( ( ( dotResult12 * 0.5 ) + 0.54 ) , _powerExp ) * _powerScale )).xxx;
				
				float3 BakedAlbedo = 0;
